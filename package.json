{
  "name": "delaford",
  "version": "0.1.0",
  "private": true,
  "engines": {
    "node": ">=22.0.0",
    "npm": ">=10.0.0"
  },
  "scripts": {
    "dev": "npm-run-all --parallel serve dev:node",
    "dev:node": "cross-env NODE_ENV=development nodemon --exec \"babel-node --config-file ./server/.babelrc\" server.js",
    "server:prod": "cross-env NODE_ENV=production node server.js",
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build --modern",
    "lint": "vue-cli-service lint",
    "start": "pm2-runtime start ecosystem.config.js --env production",
    "start:dev": "npm run dev",
    "postinstall": "node ./scripts/postinstall.js",
    "test:unit": "vue-cli-service test:unit",
    "format": "eslint --fix",
    "clean-server": "rimraf build && mkdir build",
    "transpile-server": "babel ./server --out-dir ./build --copy-files --source-maps --config-file ./server/.babelrc",
    "build-server": "npm run clean-server && npm run transpile-server",
    "lint:css": "stylelint \"src/**/*.{scss,vue}\" --custom-syntax postcss-html --allow-empty-input"
  },
  "dependencies": {
    "@babel/runtime": "^7.26.5",
    "axios": "^1.7.7",
    "compression": "^1.7.4",
    "date-fns": "^4.1.0",
    "express": "^4.21.1",
    "express-sslify": "^1.2.0",
    "lodash": "^4.17.21",
    "module-alias": "^2.2.3",
    "node-emoji": "^2.1.3",
    "pathfinding": "^0.4.18",
    "pm2": "^6.0.13",
    "ssl-express-www": "^3.2.1",
    "uuid": "^9.0.1",
    "vue": "^2.7.16",
    "vue-tippy": "^2.1.3",
    "vuex": "^3.6.2",
    "vuex-persistedstate": "^4.1.0",
    "ws": "^8.18.3"
  },
  "devDependencies": {
    "@babel/cli": "^7.26.4",
    "@babel/core": "^7.26.5",
    "@babel/eslint-parser": "^7.28.4",
    "@babel/node": "^7.26.0",
    "@babel/plugin-transform-modules-amd": "^7.26.5",
    "@babel/plugin-transform-modules-commonjs": "^7.26.5",
    "@babel/plugin-transform-modules-systemjs": "^7.26.5",
    "@babel/plugin-transform-modules-umd": "^7.26.5",
    "@babel/plugin-transform-runtime": "^7.26.5",
    "@babel/preset-env": "^7.26.5",
    "@vue/cli-plugin-babel": "^5.0.9",
    "@vue/cli-plugin-eslint": "^5.0.9",
    "@vue/cli-plugin-unit-jest": "^5.0.9",
    "@vue/cli-service": "^5.0.9",
    "@vue/eslint-config-airbnb": "^8.0.0",
    "@vue/test-utils": "^1.3.6",
    "@vue/vue2-jest": "^27.0.0",
    "babel-jest": "^27.5.1",
    "babel-plugin-root-import": "^5.1.0",
    "cross-env": "^7.0.3",
    "eslint": "^8.57.1",
    "eslint-import-resolver-babel-plugin-root-import": "^1.1.1",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-vue": "^9.30.0",
    "jest": "^27.5.1",
    "lint-staged": "^16.2.4",
    "nodemon": "^3.1.7",
    "npm-run-all": "^4.1.5",
    "postcss-html": "^1.8.0",
    "rimraf": "^6.0.1",
    "sass": "^1.93.2",
    "sass-loader": "^16.0.5",
    "stylelint": "^16.25.0",
    "stylelint-config-recommended-vue": "^1.6.1",
    "stylelint-config-standard-scss": "^16.0.0",
    "vue-template-compiler": "^2.7.16"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ],
  "nodemonConfig": {
    "env": {
      "SITE_URL": "http://website.test"
    }
  },
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "*.{js,vue}": [
      "vue-cli-service lint"
    ],
    "*.vue": [
      "stylelint --custom-syntax postcss-html --allow-empty-input"
    ]
  },
  "volta": {
    "node": "22.11.0",
    "npm": "10.9.2"
  }
}
